@page
@model IndexModel
@{
    ViewData["Title"] = "Entrance";
}

<h1>@ViewData["Title"]</h1>

<div>
    <table>
        <thead>
            <tr>
                <td>ClassRoom Name</td>
            </tr>
        </thead>
        @foreach (var classroom in Model.ClassRooms)
        {
    <tr>
        <td>@classroom.Name</td>       
        @*<td class="align-middle"><a asp-page="ClassRoom/View" asp-route-id="@classroom.Id" class="btn btn-primary">View</a></td>*@
    </tr>
        }
    </table>

</div>
